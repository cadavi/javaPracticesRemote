import java.util.*;
public class Fibonacci{
  public static void main(String args[]){
    fibonacciSerie(10);
  }
  
  public static void fibonacciSerie(int n){
    int f = 0;
    int j = 0;
    for(int i = 0; i < n; i++){
      j = f;
      if(i == 1){
         f += j + i;
         continue;
      }
      f += j + i;
      System.out.println(f);
    }
  }
}