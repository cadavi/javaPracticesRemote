import java.util.*;
public class Fibonacci{
  public static void main(String args[]){
    fibonacciSerie(10);
  }
  
  public static void fibonacciSerie(int n){
    for(int i = 0, f = 0, j = 0; i <20; i++){
      if(i <= 1){
        f += i;
        j = f;
        System.out.println(" Soy i " + i);
        continue;
      }
      f += j;
      j -= f;
      System.out.println(" " + f);
    }
  }
}